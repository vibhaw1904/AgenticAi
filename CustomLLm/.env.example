# Nugen.in API Configuration
# Copy this file to .env and add your actual credentials

# Your Nugen.in API key (required)
NUGEN_API_KEY=your-api-key-here

# Your preferred model name (optional, defaults to nugen-flash-instruct)
NUGEN_MODEL_NAME=nugen-flash-instruct

# Optional: Base URL (defaults to https://api.dev-nugen.in)
NUGEN_BASE_URL=https://api.dev-nugen.in